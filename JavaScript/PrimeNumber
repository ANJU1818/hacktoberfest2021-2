//function isPrime take a parameter and alert if it is prime or not

function isPrime(n) {
              
            var n, i, flag = true;
              
            
            n = parseInt(n)
            for(i = 2; i <= n - 1; i++)
                if (n % i == 0) {
                    flag = false;
                    break;
                }
                  
                // Check and display alert message
            if (flag == true)
                alert(n + " is prime");
            else
                alert(n + " is not prime");
        }
